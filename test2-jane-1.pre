vagrant ssh -c "cd /vagrant && mysql -u root -ppassword catDB < test2-jane-1.pre.2.sql" &> garbage.txt
echo "* empyting books table on machine 1"
cd ../machine1
vagrant up > garbage.txt
vagrant ssh -c "cd /vagrant && mysql -u root -ppassword testDB1 < test2-jane-1.pre.1.sql" &> garbage.txt
