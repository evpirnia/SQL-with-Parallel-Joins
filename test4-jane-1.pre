#!/bin/bash

echo "* initiating catalog with sailors and reserves partition info"
cd catalog
vagrant up > garbage.txt
vagrant ssh -c "cd /vagrant && mysql -u root -ppassword catDB < test4-jane-1.pre.2.sql" &> garbage.txt
echo "* initializing sailors and reserves table on machine 1"
cd ../machine1
vagrant up > garbage.txt
vagrant ssh -c "cd /vagrant && mysql -u root -ppassword testDB1 < test4-jane-1.pre.1.sql" &> garbage.txt
echo "* initializing sailors and reserves table on machine 2"
cd ../machine2
vagrant up > garbage.txt
vagrant ssh -c "cd /vagrant && mysql -u root -ppassword testDB2 < test4-jane-1.pre.3.sql" &> garbage.txt
